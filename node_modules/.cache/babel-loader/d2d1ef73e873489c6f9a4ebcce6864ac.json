ofileITLP,\n    ProfileFitness: ProfileFitness,\n    // ProfileComments,\n    ProfileCurrentWork: ProfileCurrentWork,\n    ProfileCompletedWork: ProfileCompletedWork,\n    ProfileOverviewWork: ProfileOverviewWork\n  },\n  data: function data() {\n    return {\n      currentUser: null,\n      students: [],\n      // student: null,\n      loadedStudents: false\n    };\n  },\n  methods: {\n    isDesktop: function isDesktop() {\n      return screen.width > 1200;\n    },\n    logout: function logout() {\n      var _this = this;\n\n      firebase.auth().signOut().then(function () {\n        alert(\"signed out\");\n\n        _this.$router.go({\n          path: _this.$router.path\n        });\n      });\n    }\n  },\n  watch: {\n    currentUser: function currentUser(user) {\n      this.$bind(\"students\", studentsDb.where(\"email\", \"array-contains\", user.email) // studentsDb.orderBy(\"school_year\")\n      );\n    },\n    students: function students() {\n      this.$set(this, \"loadedStudents\", true);\n    }\n  },\n  mounted: function mounted() {\n    this.$set(this, \"currentUser\", firebase.auth().currentUser); // eslint-disable-next-line\n\n    console.log(\"user is:\", firebase.auth().currentUser.displayName);\n  }\n};",{"version":3,"sources":["Dashboard.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAuCA,OAAA,GAAA,MAAA,KAAA;AACA,OAAA,KAAA,MAAA,OAAA;AACA,OAAA,sBAAA;AACA,OAAA,QAAA,MAAA,UAAA;AAEA,GAAA,CAAA,GAAA,CAAA,KAAA;AAEA,SAAA,UAAA;AACA,OAAA,MAAA;AACA,OAAA,cAAA,6B,CACA;;AACA,OAAA,cAAA;AACA,OAAA,eAAA,8B,CACA;;AACA,OAAA,kBAAA;AACA,OAAA,oBAAA;AACA,OAAA,mBAAA;AAEA,eAAA;AACA,EAAA,IAAA,EAAA,WADA;AAEA,EAAA,UAAA,EAAA;AACA,IAAA,MAAA,EAAA,MADA;AAEA,IAAA,cAAA,EAAA,cAFA;AAGA;AACA,IAAA,cAAA,EAAA,cAJA;AAKA;AACA,IAAA,kBAAA,EAAA,kBANA;AAOA,IAAA,oBAAA,EAAA,oBAPA;AAQA,IAAA,mBAAA,EAAA;AARA,GAFA;AAYA,EAAA,IAZA,kBAYA;AACA,WAAA;AACA,MAAA,WAAA,EAAA,IADA;AAEA,MAAA,QAAA,EAAA,EAFA;AAGA;AACA,MAAA,cAAA,EAAA;AAJA,KAAA;AAMA,GAnBA;AAoBA,EAAA,OAAA,EAAA;AACA,IAAA,SADA,uBACA;AACA,aAAA,MAAA,CAAA,KAAA,GAAA,IAAA;AACA,KAHA;AAIA,IAAA,MAAA,EAAA,kBAAA;AAAA;;AACA,MAAA,QAAA,CACA,IADA,GAEA,OAFA,GAGA,IAHA,CAGA,YAAA;AACA,QAAA,KAAA,CAAA,YAAA,CAAA;;AACA,QAAA,KAAA,CAAA,OAAA,CAAA,EAAA,CAAA;AAAA,UAAA,IAAA,EAAA,KAAA,CAAA,OAAA,CAAA;AAAA,SAAA;AACA,OANA;AAOA;AAZA,GApBA;AAkCA,EAAA,KAAA,EAAA;AACA,IAAA,WADA,uBACA,IADA,EACA;AACA,WAAA,KAAA,CACA,UADA,EAEA,UAAA,CAAA,KAAA,CAAA,OAAA,EAAA,gBAAA,EAAA,IAAA,CAAA,KAAA,CAFA,CAGA;AAHA;AAKA,KAPA;AAQA,IAAA,QARA,sBAQA;AACA,WAAA,IAAA,CAAA,IAAA,EAAA,gBAAA,EAAA,IAAA;AACA;AAVA,GAlCA;AA8CA,EAAA,OA9CA,qBA8CA;AACA,SAAA,IAAA,CAAA,IAAA,EAAA,aAAA,EAAA,QAAA,CAAA,IAAA,GAAA,WAAA,EADA,CAEA;;AACA,IAAA,OAAA,CAAA,GAAA,CAAA,UAAA,EAAA,QAAA,CAAA,IAAA,GAAA,WAAA,CAAA,WAAA;AACA;AAlDA,CAAA","sourcesContent":["<template>\n  <div id=\"dashboard\">\n    <div v-if=\"students.length > 0\">\n      <Navbar :student=\"students[0]\" />\n      <div v-for=\"student in students\" :key=\"student.nsn\" class=\"dashboardProfile\">\n        <ProfileDetails :overview=\"true\" :student=\"student\" class=\"margin-5 profileDetails\" />\n        <!-- <ProfileITLP :student=\"student\"/> -->\n        <!-- <ProfileComments :student=\"student\"/> -->\n        <div v-if=\"isDesktop()\" class=\"container profileWork\">\n          <ProfileCurrentWork :student=\"student\" class=\"profileCurrent\" />\n          <ProfileCompletedWork :student=\"student\" class=\"profileCompleted\" />\n        </div>\n        <div v-else>\n          <ProfileCurrentWork :student=\"student\" class=\"profileCurrent\" />\n          <ProfileCompletedWork :student=\"student\" class=\"profileCompleted\" />\n        </div>\n        <ProfileOverviewWork :student=\"student\" class=\"profileFinished\" />\n        <ProfileFitness :student=\"student\" class=\"profileFitness\" />\n        <hr />\n      </div>\n    </div>\n\n    <!-- if no student loads -->\n    <div v-if=\"students.length == 0\" class=\"flexCenter\">\n      <p>Loading...</p>\n      <br />\n      <p>\n        If page doesn't load,\n        <br />please\n        <a href=\"mailto:i.tairea@taiwananga.co.nz\">email Pa Ian</a>\n        <br />to be entered into the system.\n      </p>\n      <br />\n      <div class=\"button is-light\" id=\"signoutButton\" @click=\"logout\">Sign Out</div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Vue from \"vue\";\nimport Buefy from \"buefy\";\nimport \"buefy/dist/buefy.css\";\nimport firebase from \"firebase\";\n\nVue.use(Buefy);\n\nimport { studentsDb } from \"./firebaseInit\";\nimport Navbar from \"./Navbar.vue\";\nimport ProfileDetails from \"./ProfileDetails.vue\";\n// import ProfileITLP from \"./ProfileITLP.vue\";\nimport ProfileFitness from \"./ProfileFitness.vue\";\nimport ProfileComments from \"./ProfileComments.vue\";\n// import ProfileCommentsRuakura from \"./ProfileCommentsRuakura.vue\";\nimport ProfileCurrentWork from \"./ProfileCurrentWork.vue\";\nimport ProfileCompletedWork from \"./ProfileCompletedWork.vue\";\nimport ProfileOverviewWork from \"./ProfileOverviewWork.vue\";\n\nexport default {\n  name: \"Dashboard\",\n  components: {\n    Navbar,\n    ProfileDetails,\n    // ProfileITLP,\n    ProfileFitness,\n    // ProfileComments,\n    ProfileCurrentWork,\n    ProfileCompletedWork,\n    ProfileOverviewWork\n  },\n  data() {\n    return {\n      currentUser: null,\n      students: [],\n      // student: null,\n      loadedStudents: false\n    };\n  },\n  methods: {\n    isDesktop() {\n      return screen.width > 1200\n    },\n    logout: function() {\n      firebase\n        .auth()\n        .signOut()\n        .then(() => {\n          alert(\"signed out\");\n          this.$router.go({ path: this.$router.path });\n        });\n    }\n  },\n  watch: {\n    currentUser(user) {\n      this.$bind(\n        \"students\",\n        studentsDb.where(\"email\", \"array-contains\", user.email)\n        // studentsDb.orderBy(\"school_year\")\n      );\n    },\n    students() {\n      this.$set(this, \"loadedStudents\", true);\n    }\n  },\n  mounted() {\n    this.$set(this, \"currentUser\", firebase.auth().currentUser);\n    // eslint-disable-next-line\n    console.log(\"user is:\", firebase.auth().currentUser.displayName);\n  }\n};\n</script>\n\n<style>\n#app {\n  font-family: \"Avenir\", Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  /* margin-top: 60px; */\n}\n\n#dashboard {\n  background-color: white;\n}\n\n.margin-5 {\n  margin: 5px 0px;\n}\n.margin-20 {\n  margin: 20px 0px;\n}\n\n.profileCurrent,\n.profileCompleted,\n.profileFinished,\n.profileFitness {\n  margin-bottom: 50px;\n}\n\n.flexCenter {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n  width: 100vw;\n  height: 100vh;\n}\n\nhr {\n  margin: 50px 0px;\n}\n\n/* If viewed on computer */\n@media (min-width: 1200px) {\n  .dashboardProfile {\n    width: 100%;\n    display: flex;\n    justify-content: space-around;\n    flex-wrap: wrap;\n  }\n\n  .profileDetails {\n    width: 100%;\n  }\n\n  .profileWork {\n    order: 2;\n    width: 30%;\n    display: flex;\n    flex-direction: column;\n  }\n  .profileCurrent {width: 100%;}\n  .profileCompleted {width: 100%;}\n\n  .profileFinished {order: 1; width: 30% !important;}\n  .profileFitness {order: 3; width: 30% !important;}\n}\n</style>\n"],"sourceRoot":"src/components"}]}