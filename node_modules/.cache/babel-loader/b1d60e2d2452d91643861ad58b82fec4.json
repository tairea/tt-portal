n data() {\n    return {\n      standards: [],\n      reportComments: []\n    };\n  },\n  firestore: {// reportComments: db\n  },\n  computed: {\n    subjects: function subjects() {\n      var _this = this;\n\n      // this.subjects = [...new Set(this.standards.map(x => x.subject))]\n      var subjects = Array.from(new Set(this.standards.map(function (x) {\n        return x.subject;\n      }))).map(function (subject) {\n        return {\n          subject: subject,\n          teacher: _this.standards.find(function (x) {\n            return x.subject === subject;\n          }).teacher,\n          total: _this.getTotals(subject),\n          passed: _this.getPassed(subject)\n        };\n      });\n      return subjects;\n    },\n    getSubjectWithNoStandards: function getSubjectWithNoStandards() {\n      var arr1 = this.standards;\n      var arr2 = this.reportComments;\n      var arr3 = [].concat(arr1.filter(function (obj1) {\n        return arr2.every(function (obj2) {\n          return obj1.item !== obj2.item;\n        });\n      }), arr2.filter(function (obj2) {\n        return arr1.every(function (obj1) {\n          return obj2.item !== obj1.item;\n        });\n      }));\n      return arr3;\n    }\n  },\n  methods: {\n    filterSubjectStandards: function filterSubjectStandards(subjectName) {\n      var subStandards = this.standards.filter(function (sub) {\n        return sub.subject == subjectName;\n      }); //order the standards by this custom order\n\n      var completedOrder = [\"Not Submitted\", \"Not Achieved\", \"Achieved\", \"Merit\", \"Excellence\", \"Current\", \"Upcoming\"];\n      return subStandards.sort(function (a, b) {\n        if (a.completed == b.completed) return a.completed - b.completed;\n        return completedOrder.indexOf(a.completed) - completedOrder.indexOf(b.completed);\n      });\n    },\n    formatTeacherName: function formatTeacherName(name) {\n      //format from 'Pa Ian' to 'ian'\n      var teacherName = name.split(\" \")[1].toLowerCase(); // console.log(\"teacher name for photo is: \" + teacherName)\n\n      return teacherName;\n    },\n    getTotals: function getTotals(subjectName) {\n      var subjectStandards = this.filterSubjectStandards(subjectName); //adds all the subject assessmentCredits together\n\n      return subjectStandards.map(function (o) {\n        return o.assessmentCredits;\n      }).reduce(function (a, c) {\n        return a + c;\n      });\n    },\n    getPassed: function getPassed(subjectName) {\n      var subjectStandards = this.filterSubjectStandards(subjectName); // just get the 'completed' standards for this subject\n\n      var achievedStandards = subjectStandards.filter(function (sub) {\n        return sub.completed == \"Achieved\";\n      });\n      var meritStandards = subjectStandards.filter(function (sub) {\n        return sub.completed == \"Merit\";\n      });\n      var excellenceStandards = subjectStandards.filter(function (sub) {\n        return sub.completed == \"Excellence\";\n      });\n      var achieved = achievedStandards.map(function (o) {\n        return o.assessmentCredits;\n      }).reduce(function (a, c) {\n        return a + c;\n      }, 0);\n      var merit = meritStandards.map(function (o) {\n        return o.assessmentCredits;\n      }).reduce(function (a, c) {\n        return a + c;\n      }, 0);\n      var excellence = excellenceStandards.map(function (o) {\n        return o.assessmentCredits;\n      }).reduce(function (a, c) {\n        return a + c;\n      }, 0); //add the passed subject credits\n\n      return Number(achieved) + Number(merit) + Number(excellence);\n    },\n    getTeacherEmail: function getTeacherEmail(name) {\n      switch (name) {\n        case \"Pa Ian\":\n          return \"mailto: i.tairea@taiwananga.co.nz\";\n\n        case \"Pa Chris\":\n          return \"mailto: c.morgan@taiwananga.co.nz\";\n\n        case \"Pa Tipene\":\n          return \"mailto: t.waihi@taiwananga.co.nz\";\n\n        case \"Pa Wiremu\":\n      