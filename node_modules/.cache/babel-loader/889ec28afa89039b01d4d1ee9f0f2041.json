ate();\n      var month = date.getMonth();\n      var day = date.getDate();\n      var hour = date.getHours();\n      var minute = date.getMinutes();\n      var months = [\"Jan\", \"Feb\", \"Mar\", \"Apr\", \"May\", \"June\", \"July\", \"Aug\", \"Sept\", \"Oct\", \"Nov\", \"Dec\"];\n      return day + \" \" + months[month] + \" \" + this.pad(hour) + \":\" + this.pad(minute);\n    },\n    pad: function pad(value) {\n      if (value < 10) {\n        return '0' + value;\n      } else {\n        return value;\n      }\n    }\n  },\n  mounted: function mounted() {\n    //navbar hamburger menu\n    $(document).ready(function () {\n      // Check for click events on the navbar burger icon\n      $(\".navbar-burger\").click(function () {\n        // Toggle the \"is-active\" class on both the \"navbar-burger\" and the \"navbar-menu\"\n        $(\".navbar-burger\").toggleClass(\"is-active\");\n        $(\".navbar-menu\").toggleClass(\"is-active\");\n      });\n    });\n  }\n};",{"version":3,"sources":["Navbar.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAsCA,OAAA,GAAA,MAAA,KAAA;AACA,OAAA,KAAA,MAAA,OAAA;AACA,OAAA,sBAAA;AAEA,GAAA,CAAA,GAAA,CAAA,KAAA;AAEA,OAAA,QAAA,MAAA,UAAA;AAEA,eAAA;AACA,EAAA,IAAA,EAAA,QADA;AAEA,EAAA,KAAA,EAAA,CAAA,SAAA,CAFA;AAGA,EAAA,UAAA,EAAA,EAHA;AAIA,EAAA,IAJA,kBAIA;AACA,WAAA;AACA,MAAA,UAAA,EAAA,KADA;AAEA,MAAA,WAAA,EAAA;AAFA,KAAA;AAIA,GATA;AAUA,EAAA,OAVA,qBAUA;AACA,QAAA,QAAA,CAAA,IAAA,GAAA,WAAA,EAAA;AACA,WAAA,UAAA,GAAA,IAAA;AACA,WAAA,WAAA,GAAA,QAAA,CAAA,IAAA,GAAA,WAAA,CAAA,KAAA;AACA;AACA,GAfA;AAgBA,EAAA,OAAA,EAAA;AACA,IAAA,MAAA,EAAA,kBAAA;AAAA;;AACA,MAAA,QAAA,CACA,IADA,GAEA,OAFA,GAGA,IAHA,CAGA,YAAA;AACA,QAAA,KAAA,CAAA,YAAA,CAAA;;AACA,QAAA,KAAA,CAAA,OAAA,CAAA,EAAA,CAAA;AAAA,UAAA,IAAA,EAAA,KAAA,CAAA,OAAA,CAAA;AAAA,SAAA;AACA,OANA;AAOA,KATA;AAUA,IAAA,UAVA,sBAUA,SAVA,EAUA;AACA,UAAA,IAAA,GAAA,SAAA,CAAA,MAAA,EAAA;AACA,UAAA,KAAA,GAAA,IAAA,CAAA,QAAA,EAAA;AACA,UAAA,GAAA,GAAA,IAAA,CAAA,OAAA,EAAA;AACA,UAAA,IAAA,GAAA,IAAA,CAAA,QAAA,EAAA;AACA,UAAA,